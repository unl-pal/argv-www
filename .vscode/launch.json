    "version": "0.2.0",
    "configurations": [
        {
            "name": "PAClab: webserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "justMyCode": false,
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true
        },
        {
            "name": "PAClab: rundiscovery",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "justMyCode": false,
            "args": [
                "rundiscovery",
                "-v",
                "3",
                "--dry-run",
                "--no-poll"
            ],
            "django": true
        },
        {
            "name": "PAClab: runcloner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "justMyCode": false,
            "args": [
                "runcloner",
                "-v",
                "3",
                "--dry-run"
            ],
            "django": true
        },
        {
            "name": "PAClab: runtransforms",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "justMyCode": false,
            "args": [
                "runtransforms",
                "-v",
                "3"
            ],
            "django": true
        }
    ]
}